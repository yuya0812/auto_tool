{
  "timeout": {
    "pageLoad": 30000,
    "styleExtraction": 10000
  },
  "defaultProperties": [
    "width",
    "height",
    "min-width",
    "max-width",
    "min-height",
    "max-height",
    "margin",
    "margin-top",
    "margin-right",
    "margin-bottom",
    "margin-left",
    "padding",
    "padding-top",
    "padding-right",
    "padding-bottom",
    "padding-left",
    "display",
    "position",
    "top",
    "right",
    "bottom",
    "left",
    "flex",
    "flex-direction",
    "justify-content",
    "align-items",
    "grid-template-columns",
    "grid-template-rows",
    "gap",
    "font-size",
    "line-height",
    "color",
    "background-color",
    "border",
    "border-width",
    "border-style",
    "border-color",
    "border-radius",
    "box-shadow"
  ],
  "output": {
    "directory": "./output",
    "diffFile": "diff.json",
    "patchFile": "patch.css",
    "logFile": "log.txt"
  },
  "css": {
    "useImportant": false
  },
  "puppeteer": {
    "headless": true,
    "defaultViewport": {
      "width": 1920,
      "height": 1080
    }
  },
  "verbose": false
}
